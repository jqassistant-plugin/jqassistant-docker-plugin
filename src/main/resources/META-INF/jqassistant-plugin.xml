<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v2.4"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v2.4 https://jqassistant.github.io/jqassistant/current/schema/jqassistant-plugin-v2.4.xsd"
                    name="jQAssistant Docker Plugin" id="jqassistant.plugin.docker"
                    version="${project.version}">
    <description>${project.description}</description>
    <model>
        <class>org.jqassistant.plugin.docker.api.model.DockerRegistryDescriptor</class>
        <class>org.jqassistant.plugin.docker.api.model.DockerRepositoryDescriptor</class>
        <class>org.jqassistant.plugin.docker.api.model.DockerTagDescriptor</class>
        <class>org.jqassistant.plugin.docker.api.model.DockerManifestDescriptor</class>
        <class>org.jqassistant.plugin.docker.api.model.DockerBlobDescriptor</class>
        <class>org.jqassistant.plugin.docker.api.model.DockerLayerDescriptor</class>
        <class>org.jqassistant.plugin.docker.api.model.DockerConfigDescriptor</class>
        <class>org.jqassistant.plugin.docker.api.model.DockerContainerConfigDescriptor</class>
        <class>org.jqassistant.plugin.docker.api.model.DockerImageDescriptor</class>
        <class>org.jqassistant.plugin.docker.api.model.DockerLabelDescriptor</class>
    </model>
    <scope>
        <class>org.jqassistant.plugin.docker.api.scope.DockerScope</class>
    </scope>
    <scanner>
        <class>org.jqassistant.plugin.docker.impl.scanner.registry.DockerRegistryScannerPlugin</class>
    </scanner>
</jqassistant-plugin>
